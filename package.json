{
	"name": "ccm-plsql-oracle",
	"displayName": "PL/SQL Language Pack",
	"description": "Syntax highlighting, and snippets for Oracle PL/SQL in VS Code",
	"version": "1.2.3",
	"author": "Danil Reznichenko [@danilrez]",
	"publisher": "CrappyCodeMaker",
	"license": "MIT",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#232934",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danilrez/ccm-oracle-plsql"
	},
	"bugs": {
		"url": "https://github.com/danilrez/ccm-oracle-plsql/issues"
	},
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"plsql",
		"pl/sql",
		"oracle plsql",
		"plsql oracle",
		"pl/sql oracle",
		"oracle pl/sql",
		"oracle database",
		"oracle sql",
		"oracle developer",
		"oracle apex",
		"oracle cloud",
		"oci",
		"sqlplus",
		"stored procedure",
		"syntax highlighting",
		"plsql snippets",
		"crappycodemaker",
		"danilrez",
		"reznida"
	],
	"contributes": {
		"languages": [
			{
				"id": "plsql-ccm",
				"aliases": [
					"PL/SQL",
					"PL/SQL (Oracle)",
					"PLSQL",
					"ORACLE",
					"oracle",
					"plsql",
					"pl/sql"
				],
				"extensions": [
					".sql",
					".ddl",
					".dml",
					".pkh",
					".pks",
					".pkb",
					".pck",
					".pls",
					".plb",
					".bdy",
					".fnc",
					".idx",
					".mv",
					".prc",
					".prg",
					".sch",
					".seq",
					".spc",
					".syn",
					".tab",
					".tbl",
					".tbp",
					".tps",
					".trg",
					".typ",
					".vw"
				],
				"configuration": "./syntaxes/plsql.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "plsql-ccm",
				"scopeName": "source.plsql.oracle",
				"path": "./syntaxes/plsql.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "plsql-ccm",
				"path": "./snippets/plsql.snippets.json"
			},
			{
				"language": "plsql-ccm",
				"path": "./snippets/pldoc.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package -o ./plsql-pack-${npm_package_version}.vsix"
	}
}
